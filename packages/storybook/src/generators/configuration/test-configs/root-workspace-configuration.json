          "options": {
            "jestConfig": "libs/relib-buildable/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "reapp-swc": {
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "sourceRoot": "apps/reapp-swc/src",
      "root": "apps/reapp-swc",
